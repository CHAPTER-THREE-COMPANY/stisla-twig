  <!-- CSS Libraries -->
{#  {{ encore_entry_link_tags('app') }}#}

  <!-- CSS Libraries -->
  {% block plugins_css %}{% endblock %}

  <!-- Template CSS -->


</head>

<body>
  <div id="app">
{% block app %}{% endblock %}
  </div>

  <!-- General JS Scripts -->

  <!-- JS Libraies -->


