{% if title is not defined %}{% set title = '{{title}}' %}{% endif %}
{% if site_name is not defined %}{% set site_name = 'CHAPTER-THREE PRESENTS' %}{% endif %}
{% if version is not defined %}{% set version = 'version 2.1' %}{% endif %}
{% if menu_long is not defined %}{% set menu_long = 'STISLA' %}{% endif %}
{% if menu_short is not defined %}{% set menu_short = 'ST' %}{% endif %}
{% if page is not defined %}{% set page = 'page.html' %}{% endif %}
<!DOCTYPE html>
<html lang="jp">
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
  <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate">
  <meta http-equiv="Pragma" content="no-cache">
  <title>{{ title }} &mdash; {{ site_name }}</title>
  <link rel="icon" href="{{ path('homepage') }}favicon.ico">
  <link rel="apple-touch-icon" sizes="180x180" href="{{ path('homepage') }}apple-touch-icon.png">

  {% if ga_tracking_id != '' %}
  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id={{ga_tracking_id}}"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', '{{ga_tracking_id}}');
  </script>
  {% endif %}


  <!-- General CSS Files -->
  <!--link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous"-->
