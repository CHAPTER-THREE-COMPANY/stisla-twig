{
    "name": "chapter-three-company/c3-bundle",
    "description": "stisla for tiwg. ...",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "twig",
        "stisla"
    ],
    "homepage": "https://github.com/CHAPTER-THREE-COMPANY/stisla-twig",
    "license": "MIT",
    "authors": [
        {
            "name": "MASAO IKAWA",
            "email": "masao.ikawa@chapter-three.jp"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "aws/aws-sdk-php-symfony": "^2.7",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.*",
        "symfony/config": "^7.1",
        "symfony/console": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/filesystem": "7.1.*",
        "symfony/finder": "7.1.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.1.*",
        "symfony/notifier": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/slack-notifier": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/ux-twig-component": "^2.17",
        "symfony/yaml": "7.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.10",
        "twig/twig": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "ChapterThree\\C3Bundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
        },
        "symfony": {
            "endpoint": [
                "https://api.github.com/repos/CHAPTER-THREE-COMPANY/symfony-recipes/contents/index.json",
                "flex://defaults"
            ]
        }
    },
    "bin": ["bin/c3_info"],
    "require-dev": {
        "symfony/maker-bundle": "^1.57"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "ikawa": [
            "ChapterThree\\C3Bundle\\C3Stisla::info"
        ]
    }
}
